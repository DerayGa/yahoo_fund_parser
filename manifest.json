{
  "manifest_version": 2,

  "name": "Yahoo Fund Parser",
  "description": "Parse Yahoo Fund Data",
  "version": "1.3.4",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "default_locale" : "zh_TW",
  "permissions": [
    "*://*.yahoo.com/",
    "activeTab",
    "tabs",
    "storage"
  ]
}